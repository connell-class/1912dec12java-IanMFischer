package com.day5.collectionapi;

import java.util.HashSet;
import java.util.Set;
import java.util.TreeSet;

public class SetExample {

	public static void main(String[] args) {
		Set<String> hashSet = new HashSet<>();
		boolean first=hashSet.add("stringy");
		hashSet.add("curses");
		hashSet.add("gogo gadget hurry up");
		hashSet.add("gadgets messing up on me now!!!");
		boolean second=hashSet.add("stringy");
		System.out.println(hashSet);
		System.out.println("the first time we added stringy, the result was "+
					first+" and the second time was "+ second);
		
		Set<String> keepOhdah = new TreeSet<>();
		keepOhdah.add("on ice!!!!!!");
		keepOhdah.add("while changing the oil in his car");
		keepOhdah.add("mikey eats anything!!!");
		System.out.println(keepOhdah);
		
		
		
		
		
		
		
	}
}
